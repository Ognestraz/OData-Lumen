{
    "name": "ognestraz/odata-lumen",
    "description": "Expose Odata services from lumen framework",
    "keywords": ["laravel/lumen", "Odata", "services", "POData", "realpage"],
    "license": "MIT",
    "require": {
        "php": "~7.0",
        "algo-web/podata": "dev-master",
        "doctrine/dbal": "^2.5",
        "laravel/lumen-framework": "5.4.*",
        "illuminate/routing": "^5.4"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpunit": "5.7.*"
    },
    "autoload": {
        "psr-4": {
            "RealPage\\OData\\": "src/"
        },
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev"
}
